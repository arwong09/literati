<%= @user.username %>
<%= @user.first_name %>
<%= @user.last_name %>

<%= form_for @user do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %><br />
  
  <%= f.label :first_name %>
  <%= f.text_field :first_name %><br />
  
  <%= f.label :last_name %>
  <%= f.text_field :last_name %><br />
  
  <%= f.submit %><br />
<% end %>
